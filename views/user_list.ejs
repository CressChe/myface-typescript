<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Users</h1>
<ol>
    <% results.forEach(function(user) {%>
        <li>
            <h2><%= user.name %></h2>
            <p> 
                Username: <%= user.username%>
                <br>
                <img src=<%= user.profileImageUrl %>>
                <br> 
                Likes: <%= user.likes.length %>
                <br>
                Dislikes: <%= user.dislikes.length %>                
            </p>
        </li>
    <% }); %>
</ol>
<%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
<% } %>
<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>

</body>
<%- include('_footer') -%>
</html>